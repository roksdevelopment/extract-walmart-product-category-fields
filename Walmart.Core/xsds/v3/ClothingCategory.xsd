        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="opaque"/>
            <xsd:enumeration value="semi-opaque"/>
            <xsd:enumeration value="sheer"/>
            <xsd:enumeration value="thick opaque"/>
            <xsd:enumeration value="ultra-sheer"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="underwearStyle" type="wm:UnderwearStyle">
