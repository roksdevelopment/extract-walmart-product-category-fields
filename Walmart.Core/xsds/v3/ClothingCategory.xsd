            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Underwear Style</wm:displayName>
            <wm:group>Lower Body Features</wm:group>
            <wm:rank>99000</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="sockSize">
        <xsd:annotation>
          <xsd:documentation>Sock size. Generic sized (small/med/large) socks may not have this information (generic s/m/l size information should be entered under the clothing size attribute).</xsd:documentation>
