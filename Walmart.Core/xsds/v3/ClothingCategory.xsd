            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Hosiery Style</wm:displayName>
            <wm:group>Lower Body Features</wm:group>
            <wm:rank>97000</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="tightsSheerness">
        <xsd:annotation>
          <xsd:documentation>Sheerness/opacities specific to tights and hosiery.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Sheerness</wm:displayName>
