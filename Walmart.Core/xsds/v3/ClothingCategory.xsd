        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="shortsStyle" type="wm:ShortsStyle">
        <xsd:annotation>
          <xsd:documentation>Styles specific to shorts.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Shorts Style</wm:displayName>
            <wm:group>Lower Body Features</wm:group>
            <wm:rank>94000</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="skirtAndDressCut" type="wm:SkirtStyle">
        <xsd:annotation>
          <xsd:documentation>Terms describing a garment's skirt style, applicable to dresses and skirt products.</xsd:documentation>
