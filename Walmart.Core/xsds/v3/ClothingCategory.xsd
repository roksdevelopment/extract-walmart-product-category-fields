            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Sock Size</wm:displayName>
            <wm:group>Lower Body Features</wm:group>
            <wm:rank>100000</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="4000"/>
